{
  "name": "kicadtools",
  "version": "0.6.13",
  "description": "A lot of tools for Kicad",
  "main": "./main.js",
  "scripts": {
    "start": "electron main.js",
    "run": "react-scripts start",
    "build": "react-scripts build",
    "dist": "electron-builder"
  },
  "author": "ARNAUDIN RÃ©my",
  "build": {
    "asar": false,
    "directories": {
      "output": "build"
    },
    "publish": [
      {
        "owner": "WesterosCraftCode",
        "repo": "ElectronLauncher",
        "provider": "github"
      }
    ],
    "win": {
      "icon": "res/icons/kicad_tools.ico",
      "publish": [
        {
          "provider": "github",
          "repo": "Kicad_Tools",
          "owner": "remyar"
        }
      ],
      "artifactName": "${name}-Setup-${version}.${ext}",
      "files": [
        "!builds/*",
        "!webpack*",
        "!src/*",
        "!*.log",
        "!node_modules/*",
        "!dev-app-update.yml",
        "!.vscode/*"
      ]
    }
  },
  "dependencies": {
    "electron": "^16.0.5",
    "electron-log": "^4.4.3",
    "electron-logger": "0.0.3",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.1",
    "npm-check-updates": "^12.0.5",
    "react": "^17.0.2",
    "react-datepicker": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-intl": "^5.22.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0"
  },
  "devDependencies": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
